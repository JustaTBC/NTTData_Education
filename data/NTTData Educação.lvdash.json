        "    media_discriminacao,\r\n",
        "    media_acerto_ao_acaso,\r\n",
        "\r\n",
        "    -- 4. Contagem por classificação de dificuldade\r\n",
        "    qtd_muito_facil,\r\n",
        "    qtd_facil,\r\n",
        "    qtd_medio,\r\n",
        "    qtd_dificil,\r\n",
